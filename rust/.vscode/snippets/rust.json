  "gcd": {
    "prefix": "gcd",
    "body": [
      "fn gcd(a: u64, b: u64) -> u64 {",
      "    if b == 0 {",
      "        a",
      "    } else {",
      "        gcd(b, a % b)",
      "    }",
      "}"
    ]
  },
  "gcd_list": {
    "prefix": "gcd_list",
    "body": [
      "fn gcd(a: u64, b: u64) -> u64 {",
      "    if b == 0 {",
      "        a",
      "    } else {",
      "        gcd(b, a % b)",
      "    }",
      "}",
      "fn gcd_list(list: &[u64]) -> u64 {",
      "    list.iter().fold(list[0], |a, b| gcd(a, *b))",
      "}"
    ]
  },
  "lcm": {
    "prefix": "lcm",
    "body": [
      "fn lcm(a: u64, b: u64) -> u64 {",
      "    a / gcd(a, b) * b",
      "}"
    ]
  },
  "mymath": {
